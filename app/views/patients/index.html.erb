<h1>Patients</h1>


	<table>

		<tr>
			<th>Name</th>
			<th>Date (mm-dd-yyyy)</th>
			<th>Actions</th>

		</tr>

			<% @patients.each do |patient| %>
		<tr>
			<td><%= patient.name %></td>
			<td><%= patient.date %></td>
			<td><%= link_to "View", patient %></td>
			<td><%= link_to "Edit", edit_patient_path(patient)%></td>
			<td><%= link_to "Delete", patient, method: :delete%></td>
		</tr>


	<% end %>
	</table>

<br>

<button><%= link_to "Add patient", new_patient_path %></button>